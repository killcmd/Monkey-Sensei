        <Button x:Name="clear" Content="❌" HorizontalAlignment="Left" Margin="604,462,0,0" VerticalAlignment="Top" Click="clear_Click" Width="42" RenderTransformOrigin="0.5,0.5" Height="32" CacheMode="{DynamicResource WindowCacheMode}" >
